        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"
        android:text="用户"
        android:textColor="#FF6A6A"/>

    <TextView
        android:id="@+id/user2_ts"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dip"/>
    <Button
        android:id="@+id/user2_download"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
